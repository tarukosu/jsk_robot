<launch>
  <arg name="TO_IP" default="133.11.216.201"/>

  <!-- OCS to FC -->
  <group ns="silverhammer">
    <node pkg="jsk_network_tools" type="silverhammer_lowspeed_receiver.py"
          output="screen"
          name="ocs_lowspeed_receiver">
      <rosparam>
        message: jsk_pr2_startup/FC2OCS
        receive_port: 1025
        receive_ip: "0.0.0.0"
        topic_prefix: "/"
      </rosparam>
      <remap from="~output" to="fc2ocs_lowspeed" />
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_lowspeed_streamer.py"
          output="screen"
          name="ocs_lowspeed_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FC
        to_port: 1026
        to_ip: $(arg TO_IP)
      </rosparam>
      <remap from="~input" to="ocs2fc_original" />
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_receiver.py"
          output="screen"
          name="ocs_highspeed_receiver">
      <rosparam>
        message: jsk_pr2_startup/FC2OCSLargeData
        receive_port: 1027
        receive_ip: "0.0.0.0"
        topic_prefix: "/"
      </rosparam>
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_streamer.py"
          output="screen"
          name="ocs_highspeed_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FCLargeData
        to_port: 1028
        to_ip: $(arg TO_IP)
      </rosparam>
      <remap from="~input" to="ocs2fc_original" />
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_receiver.py"
          output="screen"
          name="ocs_heartbeat_receiver">
      <rosparam>
        message: jsk_pr2_startup/FC2OCSHeartBeat
        receive_port: 1029
        receive_ip: "0.0.0.0"
        topic_prefix: "/"
      </rosparam>
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_streamer.py"
          output="screen"
          name="ocs_heartbeat_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FCHeartBeat
        to_port: 1030
        to_ip: $(arg TO_IP)
        throttle: False
      </rosparam>
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_streamer.py"
          output="screen"
          name="ocs_joint_state_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FCJointStateRiMove
        to_port: 1031
        to_ip: $(arg TO_IP)
        throttle: False
      </rosparam>
    </node>

    
    <!-- object relation -->
    <node pkg="jsk_network_tools" type="silverhammer_highspeed_streamer.py"
          output="screen"
          name="ocs_make_model_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FCMakeModel
        to_port: 1032
        to_ip: $(arg TO_IP)
        throttle: False
      </rosparam>
    </node>

    <node pkg="jsk_network_tools" type="silverhammer_highspeed_streamer.py"
          output="screen"
          name="ocs_record_command_streamer">
      <rosparam subst_value="True">
        message: jsk_pr2_startup/OCS2FCRecordCommand
        to_port: 1033
        to_ip: $(arg TO_IP)
        throttle: False
      </rosparam>
    </node>



  </group>

  <node pkg="jsk_network_tools" type="heartbeat_responser.py"
	output="screen"
	name="heartbeat_responser"/>

</launch>
